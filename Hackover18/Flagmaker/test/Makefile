a=$(subst -,- ,$(subst +,+ ,$(1)))
q = ++--+++++ ++-+-+--+ +++---++- ++++-+-++ --+-++-++ --+++++-- -++-+--+- -+++---++ -+--+++-+ -+-++-+++ +-+-++++- +-++++-+- +---++--- +--++-+--
j=$(word $(words $(1)), $(2))
ttt=$(filter +,$(strip $(1))),$(2) +)
ss=a b c d
sss=a b c d

all:
	@echo $(call a) | tee $@
